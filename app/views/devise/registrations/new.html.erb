<div class="card-panel teal lighten-3" xmlns="http://www.w3.org/1999/html">
    <div class="container">

  <h2>Sign up</h2>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="field">
        <i class="material-icons prefix iconnegro">email</i>
        <br><%= f.label :email, class:'black-text' %><br />
        <%= f.email_field :email, autofocus: true %>
      </div>

      <div class="field">
        <i class="material-icons prefix iconnegro">lock</i>
        <br><%= f.label :password, class:'black-text'%></br>
        <% if @minimum_password_length %>
            <em>(<%= @minimum_password_length %> characters minimum)</em>
        <% end %><br />
        <%= f.password_field :password, autocomplete: "off" %>
      </div>

      <div class="field">
        <i class="material-icons prefix iconnegro">lock</i>
        <br><%= f.label :password_confirmation, class:'black-text'%><br />
        <%= f.password_field :password_confirmation, autocomplete: "off" %>
      </div>

      <div class="field">
        <i class="material-icons prefix iconnegro">account_circle </i>
        <br><%= f.label :nombre, class:'black-text' %><br />
        <%= f.text_field :name%>
      </div>

      <div class="field">

        <i class="material-icons prefix iconnegro">account_circle </i>
        <br><%= f.label :apellido,class:'black-text' %><br />
            </br>
        <%= f.text_field :lastname%>

      </div>


      <div class="field">
        <%= f.label :Fecha_de_nacimiento,class:'black-text' %><br />
        <%= f.text_field :birthdate, type: "date", class: "datepicker "%>
        <script>
            $('.datepicker').pickadate({
                selectMonths: true,
                selectYears: 170,
                selectDays:true
            });
        </script>
      </div>

      <div class="field">
        <script>$(document).ready(function() {
        $('select').material_select();
        });
        </script>
        <%=f.label :Rol,class:'black-text'%>
        <%= f.select :rol, options_for_select([["Administrador de la Organizacion", "1"], ["Ejecutivo de Ventas", "2"],["Analista", "3"]]) %>

        <label>Materialize Select</label>
      </div>




      <div class="actions">
        <%= f.submit "Sign up",class:"btn waves-effect waves-light" %>
      </div>

  <% end %>

  <%= render "devise/shared/links" %>
</div>
</div>
